<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
		  package="com.nlwl.listandphoto">

	<uses-permission android:name="android.permission.INTERNET"/>

	<application
		android:allowBackup="true"
		android:icon="@mipmap/ic_launcher"
		android:label="@string/app_name"
		android:supportsRtl="true"
		android:theme="@style/Theme.AppCompat.Light.NoActionBar">
		<activity android:name=".MainActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN"/>

				<category android:name="android.intent.category.LAUNCHER"/>
			</intent-filter>
		</activity>
		<activity android:name=".list.ListActivity">
		</activity>
		<activity android:name=".photo.PhotoActivity">
		</activity>
		<activity android:name=".photo.ChoosePhotosActivity">
		</activity>
		<activity android:name=".photo.ImageGridActivity">
		</activity>
		<activity android:name=".PostRequest.PostRequestActivity">
		</activity>
	</application>

</manifest>